{
  "name": "yard-rent",
  "version": "0.0.0",
  "scripts": {
    "prettier": "prettier --write \"**/*.{js,json,css,scss,less,md,ts,html,component.html}\"",
    "release": "standard-version",
    "commit": "git-cz",
    "ng": "ng",
    "start": "npm run svg-to-ts && ng serve --port=8888",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "i18n:extract": "transloco-keys-manager extract",
    "i18n:find": "transloco-keys-manager find",
    "svg-to-ts": "svg-to-ts -s 'src/assets/svg/*' -o 'src/app' -t SVGIcon -d KEBAB -p ''",
    "pretty:imports": "import-conductor -p @yr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.2.14",
    "@angular/cdk": "8.2.3",
    "@angular/common": "~8.2.14",
    "@angular/compiler": "~8.2.14",
    "@angular/core": "~8.2.14",
    "@angular/forms": "^8.2.14",
    "@angular/platform-browser": "~8.2.14",
    "@angular/platform-browser-dynamic": "~8.2.14",
    "@angular/router": "~8.2.14",
    "@datorama/akita": "^4.20.1",
    "@datorama/akita-cli": "^6.1.0",
    "@datorama/core": "6.24.6",
    "@datorama/themify": "^1.0.5",
    "@ngneat/content-loader": "^4.1.0",
    "@ngneat/transloco": "2.14.0",
    "@popperjs/core": "^2.4.0",
    "@schuchard/prettier": "^3.1.0",
    "ag-grid-angular": "20.2.0",
    "ag-grid-community": "20.2.0",
    "ag-grid-enterprise": "20.2.0",
    "angular-sortablejs": "^2.7.0",
    "bingmaps": "^2.0.3",
    "canvas-confetti": "^1.2.0",
    "d3": "5.12.0",
    "date-fns": "^1.30.1",
    "helpful-decorators": "^2.0.6",
    "moment": "^2.22.1",
    "ngx-semantic-version": "^1.2.1",
    "ngx-sortablejs": "^3.1.4",
    "ngx-take-until-destroy": "^5.4.0",
    "popper.js": "^1.16.1",
    "rxjs": "6.5.4",
    "sortablejs": "1.7.0",
    "tippy.js": "^6.2.3",
    "tooltip.js": "^1.3.3",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.803.25",
    "@angular/cli": "~8.3.25",
    "@angular/compiler-cli": "~8.2.14",
    "@angular/language-service": "~8.2.14",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "@ngneat/transloco-keys-manager": "^1.0.0",
    "@types/bingmaps": "0.0.1",
    "@types/d3": "^5.7.2",
    "@types/jasmine": "~3.3.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "^5.0.0",
    "cz-conventional-changelog": "^3.0.2",
    "git-cz": "^3.3.0",
    "husky": "^3.0.9",
    "import-conductor": "^1.5.1",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "lint-staged": "9.5.0",
    "ngx-build-plus": "^9.0.2",
    "prettier": "1.19.1",
    "standard-version": "^7.0.0",
    "svg-to-ts": "^3.2.1",
    "ts-node": "~7.0.0",
    "tslint": "~5.15.0",
    "tslint-config-prettier": "1.18.0",
    "typescript": "~3.5.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,less,md,ts,html,component.html}": [
      "import-conductor --staged -p @yr",
      "prettier --write",
      "git add"
    ]
  },
  "akitaCli": {
    "template": "angular"
  }
}
